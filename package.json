{
  "name": "iqair-api",
  "type": "module",
  "version": "1.0.1",
  "description": "Fully typed TypeScript SDK for scraping IQAir air quality data",
  "repository": {
    "type": "git",
    "url": "https://github.com/neo773/iqair-api.git"
  },
  "author": "neo773",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:e2e": "bun test tests/e2e",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "iqair",
    "air-quality",
    "aqi",
    "pollution",
    "pm25",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "dependencies": {
    "react-router": "^7.6.1",
    "turbo-stream": "^2.4.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
  }
}
